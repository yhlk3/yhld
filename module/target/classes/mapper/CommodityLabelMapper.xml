<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.module.commodity.mapper.CommodityLabelMapper">
<insert id="insert" parameterType="com.example.module.commodity.entity.CommodityLabelRelationship"
        useGeneratedKeys="true" keyProperty="id">
    INSERT INTO commodity_label_relationship (
    <if test="commodityLabelRelationship.commodityId != null and commodityLabelRelationship.commodityId != ''">commodity_id,</if>
    <if test="commodityLabelRelationship.labelId != null and commodityLabelRelationship.labelId != ''">label_id,</if>
    <if test="commodityLabelRelationship.createTime != null">create_time,</if>
    <if test="commodityLabelRelationship.updateTime != null">update_time,</if>
    is_deleted
    ) VALUES (
    <if test="commodityLabelRelationship.commodityId != null and commodityLabelRelationship.commodityId != ''" >#{commodityLabelRelationship.commodityId},</if>
    <if test="commodityLabelRelationship.labelId != null and commodityLabelRelationship.labelId != ''">#{commodityLabelRelationship.labelId},</if>
    <if test="commodityLabelRelationship.createTime != null">#{commodityLabelRelationship.createTime},</if>
    <if test="commodityLabelRelationship.updateTime != null">#{commodityLabelRelationship.updateTime},</if>
    #{commodityLabelRelationship.isDeleted}
    )
</insert>
</mapper>